<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\Source" table="source">
    <unique-constraints>
      <unique-constraint name="source_pkey" columns="id"/>
      <unique-constraint name="un_source_code" columns="id"/>
    </unique-constraints>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="source_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="code" type="string" column="code" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="libelle" type="string" column="libelle" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ordre" type="integer" column="ordre" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>

    <field name="importable" type="boolean" column="importable" nullable="false"/>
    <field name="lock" type="boolean" column="lock" nullable="false"/>
    <field name="synchro_delete_enabled" type="boolean" column="synchro_delete_enabled" nullable="false"/>
    <field name="synchro_insert_enabled" type="boolean" column="synchro_insert_enabled" nullable="false"/>
    <field name="synchro_undelete_enabled" type="boolean" column="synchro_undelete_enabled" nullable="false"/>
    <field name="synchro_update_enabled" type="boolean" column="synchro_update_enabled" nullable="false"/>
  </entity>
</doctrine-mapping>
